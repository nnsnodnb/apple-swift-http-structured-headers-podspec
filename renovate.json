{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^Mintfile$"],
      "matchStrings": [
        "^(?<depName>[^ ]+) (?<currentValue>[0-9.]+)$"
      ],
      "datasourceTemplate": "github-releases"
    }
  ]
}
